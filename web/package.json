{
  "name": "wgpu-canvas-editor-web",
  "version": "0.1.0",
  "description": "Web frontend for WGPU Canvas Editor",
  "scripts": {
    "wasm:build": "cd .. && wasm-pack build --target web --out-dir web/pkg",
    "wasm:dev": "cd .. && wasm-pack build --target web --dev --out-dir web/pkg",
    "build": "npm run wasm:build && webpack --mode production",
    "dev": "npm run wasm:dev && webpack serve --mode development --open",
    "start": "npm run dev",
    "serve": "webpack serve --mode development --open",
    "clean": "rm -rf dist pkg"
  },
  "devDependencies": {
    "@wasm-tool/wasm-pack-plugin": "^1.7.0",
    "copy-webpack-plugin": "^12.0.2",
    "html-webpack-plugin": "^5.6.0",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.1.0"
  },
  "keywords": [
    "wgpu",
    "webassembly",
    "rust",
    "canvas",
    "editor"
  ],
  "author": "cheolcheol",
  "license": "MIT"
}
